for(int i=0;i<n;i++){
        int min = i;
        for(int j=i+1;j<n;j++){
            if(a[j]<a[min]) min = j;
        }
        int temp  = a[i];
        a[i] = a[min];
        a[min] = temp;
 }
 //Algorithm Analysis
 // Worst , best , average Case Time Complexity - O(n-square)
 // Space Complexity = O(1)
 // Stablity - Not stable (depends on implentation)
