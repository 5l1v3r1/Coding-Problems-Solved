for(int i=0;i<n;i++){
      for(int j=0;j<n-1;j++){
           if(a[j]>a[j+1]){ 
                int temp = a[j];
                a[j] = a[j+1];
                a[j+1] = temp;
              }
          }
}
//Algorithm Analysis
// Worst Case Time Complexity - O(n-square)
// Average Case Time Complexity - O(n-square)
// Space Complexity - O(n)
